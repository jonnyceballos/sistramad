<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="panel panel-info">
      <div class="panel-heading center">Reference list</div>
      <div class="panel-body">
        <p>
          <strong>Name:</strong>
          <%= @reference_list.name %>
        </p>

        <p>
          <strong>Description:</strong>
          <%= @reference_list.description %>
        </p>

        <p>
          <strong>Value:</strong>
          <%= @reference_list.value %>
        </p>

        <p>
          <strong>Reference:</strong>
          <%= @reference_list.reference.name %>
        </p>
      </div>
    </div>
  </div>
</div>

<%= link_to 'Edit', edit_reference_list_path(@reference_list) %> |
<%= link_to 'Back', reference_lists_path %>
