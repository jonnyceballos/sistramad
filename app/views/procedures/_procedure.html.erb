
<div class="table-responsive">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Solicitud </th>
        <th>Fecha de solicitud</th>
        <th colspan="4"></th>
      </tr>
    </thead>

    <tbody>
      <% @procedures.each do |procedure| %>
          <tr>
            <td> <%= link_to procedure.name, procedure_path(procedure) %> </td>
            <td><%= procedure.created_at %></td>
            <td><%= procedure.created_at %></td>
            <td>
              <div class="btn-toolbar">
                <div class="btn-group btn-group-xs">
                  <%= link_to 'Consultar' ,procedure_path(procedure), class: 'btn btn-primary list' %>
                  <%= link_to 'Cancelar', procedure_path(procedure), method: :delete, data: {confirm: '¿Está seguro de elimniar solicitud?'}, class: 'btn btn-danger list' %>
                </div>
              </div>
            </td>
          </tr>
      <% end %>
    </tbody>
  </table>
</div>

