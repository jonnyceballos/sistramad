<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="panel panel-info">
      <div class="panel-heading center">Reference list</div>
      <div class="panel-body">
        <label for="username" class="control-label">username</label>
        <input type="text" class="form-control" value=<%= @user.username %> disabled= true>

        <label for="first_name" class="control-label">first_name</label>
        <input type="text" class="form-control" value=<%= @user.first_name %> disabled= true>

        <label for="middle_name" class="control-label">middle_name</label>
        <input type="text" class="form-control" value=<%= @user.middle_name %> disabled= true>

        <label for="last_name" class="control-label">last_name</label>
        <input type="text" class="form-control" value=<%= @user.last_name %> disabled= true>

        <label for="genre" class="control-label">genre</label>
        <input type="text" class="form-control" value=<%= @user.genre %> disabled= true>

        <label for="identification_document" class="control-label">identification_document</label>
        <input type="text" class="form-control" value=<%= @user.identification_document %> disabled= true>

        <label for="birthday" class="control-label">birthday</label>
        <input type="text" class="form-control" value=<%= @user.birthday %> disabled= true>

        <label for="email" class="control-label">email</label>
        <input type="text" class="form-control" value=<%= @user.email %> disabled= true>
      </div>
    </div>
  </div>
</div>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
